{
  "name": "vendobox-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --max-warnings 0 src/ && yarn ts && yarn lint:css",
    "lint:ci": "eslint --max-warnings 0 src/",
    "ts": "tsc --noEmit",
    "lint:css": "stylelint './src/**/*.{ts,js}'",
    "lint:fix": "eslint --fix --max-warnings 0 src/",
    "css:build": "node-sass src/ -o src/",
    "css:watch": "npm run css:build && sass src/ -o src/ --watch --recursive",
    "js:start": "react-app-rewired start",
    "start": "npm-run-all -p js:start",
    "dev": "PORT=3001 yarn start",
    "start:local": "REACT_APP_WEBSITE_DOMAIN=http://localhost:3000 REACT_APP_API=http://localhost:4000/graphql PORT=3001 npm-run-all -p js:start",
    "start:dev": "PORT=3001 REACT_APP_API=http://localhost:4001/graphql npm-run-all -p css:watch js:start",
    "start:prod": "REACT_APP_API=https://api.shakeelengineering.com.au/graphql npm-run-all -p css:watch js:start",
    "js:build": "CI=true react-app-rewired build",
    "build": "npm-run-all css:build js:build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "load-schema": "NODE_TLS_REJECT_UNAUTHORIZED=0 && yarn apollo service:download --endpoint=http://localhost:4000/graphql utils/graphql-schema.json",
    "apollo-codegen": "yarn apollo codegen:generate --localSchemaFile=utils/graphql-schema.json --target=typescript --tagName=gql --includes=\"src/**/*.{ts,tsx}\" --outputFlat src/schema",
    "generate-apollo-types": "yarn load-schema && yarn apollo-codegen"
  },
  "dependencies": {
    "@apollo/client": "3.4.15",
    "@date-io/moment": "^1.3.9",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/pickers": "3.3.10",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.2",
    "@mui/styles": "^5.11.2",
    "bootstrap": "^4.1.3",
    "chart.js": "^2.9.4",
    "color": "3.1.3",
    "debounce-promise": "^3.1.2",
    "formik": "^2.2.9",
    "graphql": "15.6.0",
    "isomorphic-unfetch": "3.1.0",
    "loaders.css": "^0.1.2",
    "lodash": "4.17.21",
    "moment": "2.29.2",
    "moment-timezone": "0.5.33",
    "node-sass": "^7.0.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "qs": "6.10.1",
    "query-string": "7.0.1",
    "react": "17.0.2",
    "react-content-loader": "^3.1.2",
    "react-copy-to-clipboard": "5.0.3",
    "react-date-picker": "^7.7.0",
    "react-dates": "^21.3.1",
    "react-datetime-picker": "^2.6.0",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-loadable": "^5.5.0",
    "react-loaders": "^3.0.1",
    "react-loading-skeleton": "2.2.0",
    "react-responsive": "^8.0.1",
    "react-router-dom": "5.3.0",
    "react-s3-uploader": "^4.8.0",
    "react-scripts": "4.0.3",
    "react-scroll": "^1.7.10",
    "react-select": "5.2.2",
    "react-simple-oauth2-login": "^0.5.0",
    "reactstrap": "^6.3.1",
    "recompose": "^0.30.0",
    "rrweb": "0.9.14",
    "sass": "^1.50.1",
    "styled-components": "5.3.3",
    "typescript": "^4.5.4",
    "use-query-params": "^0.3.3",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.0",
    "@playlyfe/gql": "^2.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "12.1.1",
    "@types/color": "^3.0.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@types/numeral": "^2.0.2",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.37",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dates": "^21.8.3",
    "@types/react-dom": "^17.0.11",
    "@types/react-geosuggest": "^2.7.13",
    "@types/react-helmet": "^6.1.5",
    "@types/react-loadable": "^5.5.6",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-scroll": "^1.8.3",
    "@types/reactstrap": "^8.7.2",
    "@types/recompose": "^0.30.10",
    "@types/styled-components": "^5.1.18",
    "abortcontroller-polyfill": "^1.5.0",
    "apollo": "^2.33.9",
    "babel-plugin-import": "1.13.3",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-styled-components-a11y": "^0.0.34",
    "eslint-plugin-xss": "^0.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^13.0.0",
    "jest-styled-components": "^7.0.4",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "react-app-rewired": "2.1.8",
    "react-test-renderer": "^16.12.0",
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^27.1.2",
    "webpack-merge": "^5.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/stories",
      "<rootDir>/src/setupTests.js"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --max-warnings 0",
      "yarn test --passWithNoTests --bail --watchAll=false"
    ]
  },
  "sideEffects": [
    "isomorphic-unfetch",
    "react-dates/initialize"
  ],
  "resolutions": {
    "babel-loader": "8.1.0",
    "graphql": "15.6.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
